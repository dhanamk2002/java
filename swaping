public class swaping
 {
   public static void main(String []args)
    {
      int a=03;
      int b=16;
      System.out.println("a is" +a+ and "b is" +b);
      a=a+b;
      b=a-b;
      a=a-b;
    System.out.println("After Swaping, a is +a+ "and b is"+b);
   }
}
//output
// a is 03 and b is 16 After Swaping, a is 16 and b is 03
      
